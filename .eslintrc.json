{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "extends": ["plugin:@docusaurus/recommended"],
  "plugins": ["@docusaurus"],
  "rules": {
    "@docusaurus/string-literal-i18n-messages": "error",
    "@docusaurus/no-html-links": "error"
  },
  "ignorePatterns": ["node_modules/", "build/"]
}
